{
  "name": "billing-migrator",
  "version": "1.0.0",
  "description": "User-friendly billing migration tool",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"cd frontend && npm run start\"",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd frontend && npm install",
    "setup": "npm install && cd frontend && npm install && npm run build"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "braintree": "^3.16.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^3.3.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.0.1"
  }
}
